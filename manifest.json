{
  "manifest_version": 3,
  "name": "Maps Confidence Level",
  "version": "1.0",
  "description": "Provides a confidence level for places on Google Maps.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.google.com/maps/*"
  ]
}
